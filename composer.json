{
    "name": "sonata-project/sandbox",
    "description": "Sonata Project's sandbox (with ecommerce bundles)",
    "license": "MIT",

    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-bcmath": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "friendsofsymfony/comment-bundle": "^2.3",
        "friendsofsymfony/rest-bundle": "^2.5",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^3.4",
        "kriswallsmith/buzz": "0.16",
        "liip/monitor-bundle": "^2.9.1",
        "mopa/bootstrap-bundle": "*",
        "nelmio/api-doc-bundle": "2.*",
        "sensio/framework-extra-bundle": "^5.4",
        "simplethings/entity-audit-bundle": "1.0",
        "sonata-project/admin-bundle": "^3.54",
        "sonata-project/block-bundle": "^3.18",
        "sonata-project/cache-bundle": "^3.1",
        "sonata-project/classification-bundle": "^3.9",
        "sonata-project/comment-bundle": "^3.1",
        "sonata-project/core-bundle": "^3.17",
        "sonata-project/doctrine-extensions": "^1.3",
        "sonata-project/doctrine-orm-admin-bundle": "^3.10",
        "sonata-project/easy-extends-bundle": "^2.5",
        "sonata-project/ecommerce": "master",
        "sonata-project/exporter": "^2.0",
        "sonata-project/google-authenticator": "^2.2",
        "sonata-project/intl-bundle": "^2.7",
        "sonata-project/media-bundle": "^3.20",
        "sonata-project/news-bundle": "^3.10",
        "sonata-project/notification-bundle": "^3.6",
        "sonata-project/page-bundle": "^4.0@dev",
        "sonata-project/seo-bundle": "^2.7",
        "sonata-project/timeline-bundle": "^3.4",
        "sonata-project/user-bundle": "^4.5",
        "symfony/acl-bundle": "^1.0",
        "symfony/config": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/serializer": "4.3.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/webpack-encore-bundle": "^1.6.2",
        "symfony/yaml": "4.3.*",
        "twbs/bootstrap": "v3.0.0",
        "willdurand/faker-bundle": "^1.4"
    },
    "require-dev": {
        "behat/behat": "^3.5",
        "behat/common-contexts": "^1.2",
        "behat/mink": "~1.7",
        "behat/mink-browserkit-driver": "~1.3",
        "behat/mink-extension": "~2.3",
        "behat/mink-goutte-driver": "~1.0",
        "behat/mink-selenium2-driver": "~1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "guzzle/guzzle": "~3.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "dev-master",
        "symfony/web-server-bundle": "4.3.*"
    },

    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Sonata\\": "Sonata/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
